<center>
  <div>
    <p id="notice"><%= notice %></p>

    <h1>Comment Feedbacks</h1>

    <table>
      <thead>
        <tr>
          <th>Discussion forum</th>
          <th>Body</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @comment_feedbacks.each do |comment_feedback| %>
          <tr>
            <td><%= comment_feedback.discussion_forum_id %></td>
            <td><%= comment_feedback.body %></td>
            <td><%= link_to 'Show', comment_feedback %></td>
            <td><%= link_to 'Edit', edit_comment_feedback_path(comment_feedback) %></td>
            <td><%= link_to 'Destroy', comment_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Comment Feedback', new_comment_feedback_path %>
  </div>
</center>